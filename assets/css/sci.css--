}

.article-detail img {
    width: auto;
    max-width: 100%;
    margin: 10px auto 20px;
    text-align: center;
    float: left;
    padding-right: 15px;
}

.video-div {
    position: relative;
    width: 100%;
    padding-top: 56.25%;
}

.video-div .video-content {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
}

.video-div video {
    width: 100%;
    height: 100%;
}

.video-js {
    width: 100% !important;
    height: 100% !important;
}

.embed-container {
    position: relative;
    padding-bottom: 56.25%;
    padding-top: 30px;
    height: 0;
    overflow: hidden;
    max-width: 100%;
    height: auto;
}

.embed-container iframe,
.embed-container object,
.embed-container embed {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}


/*border*/

.bd-r {
    border-right: 1px dashed #0D47A1;
}

.bd-l {
    border-left: 1px dashed #0D47A1;
}


/*div*/

.div-section {
    background: #fff;
    margin-top: 20px;
    padding: 25px;
}

.div-section:first-of-type {
