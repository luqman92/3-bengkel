}

.article-list {
    margin: 0 0 30px 0;
}

.sidebar-list {
    padding: 0;
    margin-bottom: 15px;
}

.vod-thumbnail {
    background: rgba(62, 61, 61, 0.4);
    color: #fff;
    position: absolute;
    width: 100%;
    top: 0;
    bottom: 0;
    text-align: center;
}

.vod-thumbnail i {
    margin-top: 20%;
    -webkit-transition: all 100ms linear;
    -moz-transition: all 100ms linear;
    -o-transition: all 100ms linear;
    -ms-transition: all 100ms linear;
    transition: all 100ms linear;
}

.vod-thumbnail i:hover {
    -moz-transform: scale(1.5);
    -webkit-transform: scale(1.5);
    -o-transform: scale(1.5);
    transform: scale(1.5);
}

.podcast-thumbnail {
    background: #F77E00;
    color: #fff;
    padding: 5px;
    width: 35px;
    text-align: center;
    float: left;
    position: absolute;
    z-index: 2;
    margin-top: -31px;
}

.breadcrumb {
