}

.newsletter-div {
    background: #CED0D1;
    padding: 5px 15px;
    position: fixed;
    bottom: 0;
    width: 100%;
    z-index: 3;
}

.adspot-1 {
    width: 100%;
    text-align: center;
    margin-top: 20px;
}

.adspot-2 {
    margin-top: 30px;
}

.adspot-1 img {
