}

.ellipsis-text {
    white-space: nowrap;
    overflow: hidden;
    -moz-text-overflow: ellipsis;
    text-overflow: ellipsis;
}

.ellipsis-clamp {
    display: block;
    display: -webkit-box;
    max-width: 100%;
    height: 46px;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.ellipsis-clamp-3 {
    display: block;
    display: -webkit-box;
    max-width: 100%;
    height: 63px;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.clickable {
    cursor: pointer;
}

.mobile {
    display: none;
}

.desktop {
    display: block;
}

@media screen and (max-width: 1024px) {
    .desktop {
        display: none;
    }
    .mobile {
        display: block;
    }
    html {
        margin-top: 60px;
    }
    body {
        margin-bottom: 0px !important;
        margin-top: 44px;
    }
    .sci-header-mobile {
        text-align: center;
        color: #fff;
        padding: 0;
        position: fixed;
        width: 100%;
        z-index: 5;
        top: 0;
    }
    .sci-header-mobile .top {
        font-size: 18px;
        line-height: 60px;
        background: #000;
        height: 60px;
    }
    .search-mobile-div {
        display: none;
        padding: 10px;
    }
    .mobile-nav>ul li {
        display: inline-block;
        float: left;
        border-bottom: 4px solid #fff;
    }
    .mobile-nav>ul li:nth-child(even) {
        width: 27%;
    }
    .mobile-nav>ul li:nth-child(odd) {
        width: 23%;
    }
    .mobile-nav>ul .active {
        border-bottom: 4px solid #0029DB;
    }
    .mobile-nav>ul li a {
        display: block;
        position: relative;
        overflow: hidden;
        padding: 12px 4px 10px;
        text-decoration: none;
        text-align: center;
        font-weight: bold;
        color: #0029DB;
        font-size: 13px;
        background: #fff;
    }
    .div-section {
        padding: 15px;
    }
    .div-section:first-of-type {
        padding-top: 0;
    }
    .col-80-m {
        width: 80px;
        float: left;
    }
    .col-80-f-m {
        width: calc(100% - 80px);
        float: left;
    }
    .img-80-m {
        height: 80px;
        width: 80px;
        object-fit: cover;
    }
    .pad-0-25 {
        padding: 0 15px;
    }
    .pad-img,
    .pad-0-m {
        padding: 0;
    }
    .pad-0-15-m {
        padding: 0 15px;
    }
    .pad-15-m {
        padding: 15px;
    }
    .pad-0-5-m {
        padding: 0 5px;
    }
    .mg-0-m {
        margin: 0;
    }
    .mt-min-30 {
        margin-top: 0;
    }
    .mt-5-m {
        margin-top: 5px;
    }
    .mt-10-m {
        margin-top: 10px;
    }
    .mb-10-m {
        margin-bottom: 10px;
    }
    .article-title {
        position: absolute;
        z-index: 1;
        background: rgba(0, 0, 0, 0.8);
        width: calc(100% - 30px);
        right: 15px;
        height: 130px;
        padding: 15px;
        bottom: 0;
    }
    .article-title .headline-1 {
        color: #fff !important;
        font-size: 20px;
        font-weight: 700;
    }
    .headline-2 {
        font-size: 20px;
    }
    .headline-3 {
        font-size: 18px;
    }
    .sci-footer .menu-title {
        font-size: 20px;
        text-align: center;
        margin-bottom: 10px;
    }
    .pagination>li>a,
    .pagination>li>span {
        padding: 5px 12px;
        margin-left: 10px;
    }
    .footer-mobile-swap {
        display: -webkit-box;
        display: -moz-box;
        display: -ms-flexbox;
        display: -webkit-flex;
        display: flex;
        -webkit-flex-flow: row wrap;
        flex-flow: row wrap;
    }
    .footer-explore {
        width: 240px;
    }
    .footer-about {
        order: 3;
    }
    .footer-contact {
        order: 2;
        font-size: 13px;
        max-width: 320px;
        margin-left: auto;
        margin-right: auto;
    }
    .modal.left .modal-dialog {
        position: fixed;
        margin: auto;
        width: 280px;
        height: 100%;
        -webkit-transform: translate3d(0%, 0, 0);
        -ms-transform: translate3d(0%, 0, 0);
        -o-transform: translate3d(0%, 0, 0);
        transform: translate3d(0%, 0, 0);
    }
    .modal.left .modal-content {
        height: 100%;
        overflow-y: auto;
    }
    .modal.left .modal-body {
        padding: 15px 15px 80px;
    }
    /*Left*/
    .modal.left.fade .modal-dialog {
        left: -320px;
        -webkit-transition: opacity 0.3s linear, left 0.3s ease-out;
        -moz-transition: opacity 0.3s linear, left 0.3s ease-out;
        -o-transition: opacity 0.3s linear, left 0.3s ease-out;
        transition: opacity 0.3s linear, left 0.3s ease-out;
    }
    .modal.left.fade.in .modal-dialog {
        left: 0;
    }
    /* ----- MODAL STYLE ----- */
    .modal-content {
        border-radius: 0;
        border: none;
        background: #0029DB;
        color: #fff;
    }
    .modal-header {
        color: #fff;
        background-color: #000;
    }
    .modal-body .nav>li>a {
        color: #fff;
        font-weight: bold;
        font-size: 18px;
    }
    .scrollup {
        right: 20px;
        bottom: 20px;
    }
