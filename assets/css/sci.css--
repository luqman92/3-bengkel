html,
body {
    background: #f2f2f2;
    font-family: 'Source Sans Pro', sans-serif !important;
}

.navbar-default {
    background: #0D47A1;
    margin-bottom: 0;
    border: none;
    top: -30px;
    z-index: 5;
}

.scrollup {
    width: 50px;
    height: 50px;
    position: fixed;
    bottom: 80px;
    right: 80px;
    display: none;
    background-color: rgba(0, 0, 0, 0.5);
    color: #fff;
    line-height: 50px;
    text-align: center;
}

.scrollup:hover {
    color: #fff;
}

.nav-up {
    top: -40px;
}

.content {
    background: #fff;
    padding: 2em;
}


/*link*/

a:hover,
a:focus {
    text-decoration: none;
}

.headline-1 {
    margin: 0 0 5px;
    font-size: 28px;
    font-weight: 700;
    color: #333;
    line-height: 1.2em;
}

.headline-2 {
    margin: 0 0 5px;
    font-size: 28px;
    font-weight: 600;
    color: #333;
    line-height: 1.2em;
}

.headline-3 {
    margin: 0 0 5px;
    font-size: 20px;
    font-weight: 600;
    color: #333;
    line-height: 1.2em;
}

.headline-1:hover,
.headline-1:focus,
.headline-2:hover,
.headline-2:focus,
.headline-3:hover,
.headline-3:focus,
.sidebar-link:hover,
.sidebar-link:focus {
    color: #0029DB;
}

.sidebar-link {
    font-size: 16px;
    font-weight: 600;
    line-height: 1.3;
    color: #333;
    margin-bottom: 3px;
}

.postlink-detail {
    font-size: 13px;
    color: #b5b5b5;
}

.menu-title {
    font-size: 22px;
    margin: 0 0 10px;
    font-weight: 600;
    margin-bottom: 6px;
}

.pagination>li>a,
.pagination>li>span {
    margin-left: 15px;
    color: #333;
    font-weight: 600;
}

.pagination>li>a:hover,
.pagination>li>span:hover,
.pagination>li>a:focus,
.pagination>li>span:focus {
    color: #ffffff;
    background-color: rgba(174, 2, 2, 0.8);
    border-color: rgba(174, 2, 2, 0.8);
}

.pagination>.active>a,
.pagination>.active>span,
.pagination>.active>a:hover,
.pagination>.active>span:hover,
.pagination>.active>a:focus,
.pagination>.active>span:focus {
    color: #ffffff;
    background-color: #ae0202;
    border-color: #ae0202;
}

.pagination>li:first-child>a,
.pagination>li:first-child>span,
.pagination>li:last-child>a,
.pagination>li:last-child>span {
    border: none;
}


/*font*/

.font-blue {
    color: #0029DB;
}

.font-red {
    color: #AE0202;
}

.font-white {
